{
  "manifest_version": 2,
  "name": "Online-Offline",
  "version": "0.1" ,
  "description" : "Tells you whether you are online or offline" ,
  "background" : {
  		"scripts" : ["jquery-3.1.0.min.js" , "background.js"]
  	},
	 
	"browser_action": {
	  "default_icon": "icon.png", 
	  "default_title": "Status: "
	} ,

	"icons": {
		"64" : "icon.png" , 
		"32" : "icon.png" , 
		"16" : "icon.png" , 
		"128" : "icon.png"
	} , 
	"permissions" : [
		"*://www.google.com/*" , "*://www.google.co.in/*"
	]
}